function bmiCalculator (weight, height) {
    var bmi= (weight)/(height*height);
    var interprator = "";
    if (bmi<18.5) {
    interprator = `Your BMI is ${bmi}, so you are underweight.`;
    }
    if (bmi>18.5 && bmi<24.9) {
    interprator =`Your BMI is ${bmi}, so you have a normal weight.`;
    }
    if (bmi>24.9) {
    interprator = `Your BMI is ${bmi}, so you are overweight.`;
    }
    return interprator;
    }